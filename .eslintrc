{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "max-len": ["error" , {
      "code": 120
    }],
    "operator-linebreak": "off",
    "require-jsdoc": "off",
    "semi": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}
